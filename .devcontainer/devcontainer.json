{
    "name": "db_schema",
    "dockerComposeFile": [
        "../compose.yml"
    ],
    "service": "db_schema",
    "workspaceFolder": "/workspace",
    "features": {
        "ghcr.io/devcontainers/features/docker-outside-of-docker:1": {
            "version": "latest",
            "moby": true,
            "dockerDashComposeVersion": "v2"
        },
        "ghcr.io/devcontainers/features/aws-cli:1": {},
        "ghcr.io/devcontainers/features/git:1": {}
    },
    "mounts": [
        "source=/var/run/docker.sock,target=/var/run/docker-host.sock,type=bind",
        "source=${localEnv:HOME}/.gitconfig,target=/etc/gitconfig,type=bind,consistency=cached",
        "source=${localEnv:HOME}/.aws,target=/home/developer/.aws,type=bind,consistency=cached"
    ],
    "customizations": {
        "vscode": {
            "extensions": [
                "aaron-bond.better-comments",
                "bibhasdn.unique-lines",
                "chunsen.bracket-select",
                "wmaurer.change-case",
                "streetsidesoftware.code-spell-checker",
                "esbenp.prettier-vscode",
                "njpwerner.autodocstring",
                "oderwat.indent-rainbow",
                "RomanPeshkov.vscode-text-tables",
                "usernamehw.errorlens",
                "xyz.local-history",
                "ryanluker.vscode-coverage-gutters",
                "shardulm94.trailing-spaces",
                "ms-azuretools.vscode-docker",
                "mhutchie.git-graph",
                "eamodio.gitlens",
                "waderyan.gitblame",
                "donjayamanne.githistory",
                "shd101wyy.markdown-preview-enhanced",
                "yzhang.markdown-all-in-one",
                "TakumiI.markdowntable",
                "fcrespo82.markdown-table-formatter",
                "csholmq.excel-to-markdown-table",
                "bierner.markdown-mermaid",
                "oderwat.indent-rainbow",
                "adpyke.vscode-sql-formatter",
                "matangover.mypy",
                "donjayamanne.python-extension-pack",
                "littlefoxteam.vscode-python-test-adapter",
                "ms-python.black-formatter",
                "ms-python.flake8",
                "ms-python.isort",
                "ms-python.python",
                "njpwerner.autodocstring",
                "amazonwebservices.aws-toolkit-vscode",
                "bpruitt-goddard.mermaid-markdown-syntax-highlighting",
                "GitHub.copilot",
                "tamasfe.even-better-toml"
            ],
            "settings": {
                "python.venvPath": "${workspaceFolder}/.venv",
                "python.defaultInterpreterPath": "${workspaceFolder}/.venv/bin/python",
                "python.testing.pytestPath": "${workspaceFolder}/.venv/bin/pytest",
                "flake8.path": [
                    "${workspaceFolder}/.venv/bin/pflake8"
                ],
                "flake8.args": [
                    "--config=${workspaceFolder}/pyproject.toml"
                ],
                "python.formatting.blackPath": "${workspaceFolder}/.venv/bin/black",
                "black-formatter.args": [
                    "--config=${workspaceFolder}/pyproject.toml"
                ],
                "isort.path": [
                    "${workspaceFolder}/.venv/bin/isort"
                ],
                "isort.interpreter": [
                    "${workspaceFolder}/.venv/bin/python"
                ],
                "[python]": {
                    "editor.defaultFormatter": "ms-python.black-formatter",
                    "editor.formatOnSave": true,
                    "editor.codeActionsOnSave": {
                        "source.organizeImports": "explicit"
                    }
                },
                "mypy.dmypyExecutable": "${workspaceFolder}/.venv/bin/dmypy",
                "python.testing.pytestEnabled": true,
                "python.testing.autoTestDiscoverOnSaveEnabled": true,
                "python.testing.pytestArgs": [
                    ".",
                    "--cov=src",
                    "--cov-report",
                    "xml",
                    "-vv"
                ],
                "coverage-gutters.showGutterCoverage": false,
                "coverage-gutters.showLineCoverage": true,
                "coverage-gutters.showRulerCoverage": true,
                "files.insertFinalNewline": true,
                "files.trimTrailingWhitespace": true
            }
        }
    }
}
